
{{ form_start (form) }}
<div class="row">
    <div class="col-md-4">{{ form_row(form.username) }}</div>
    <div class="col-md-4">{{ form_row(form.email) }}</div>
</div>
<div class="row">
    <div class="col-md-2">{{ form_row(form.firstname) }}</div>
    <div class="col-md-2">{{ form_row(form.lastname) }}</div>
    <div class="col-md-2">{{ form_row(form.admin) }}</div>
</div>
{{ form_rest (form) }}
<button class="btn btn-primary">{{ button|default('Enrengistrer') }}</button>
{{ form_end (form) }}